{
  "name": "webdriverio-typescript-samples",
  "version": "3.0.2",
  "description": "Provides examples of how to use TypeScript with the webdriver.io api. Tests executed against http://the-internet.herokuapp.com..",
  "main": "index.js",
  "scripts": {
    "e2e.android": "npm run build && npx wdio wdio.conf.js android",
    "e2e.ios": "npm run build && npx wdio wdio.conf.js ios",
    "build": "rimraf lib && tsc --outDir lib"
  },
  "dependencies": {
    "browserstack-local": "1.4.8",
    "webdriverio": "^7.7.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@wdio/cli": "^7.7.4",
    "@wdio/dot-reporter": "^7.7.3",
    "@wdio/local-runner": "^7.7.4",
    "@wdio/mocha-framework": "^7.7.4",
    "@wdio/selenium-standalone-service": "^7.7.4",
    "@wdio/spec-reporter": "^7.7.3",
    "@wdio/sync": "^7.7.4",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "it-each": "^0.4.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.3.4"
  }
}
